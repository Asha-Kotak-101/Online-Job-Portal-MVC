@model List<Online_Job_Portal_MVC.Models.UserListModel>

@{
    ViewData["Title"] = "Contact List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Contact List</h1>

@if (TempData["msg"] != null)
{
    <div class="alert alert-success">@TempData["msg"]</div>
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Mobile No.</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.GetUser.Username</td>
                <td>@item.GetUser.Email</td>
                <td>@item.GetUser.MobileNumber</td>
                <td>@item.GetUser.Country</td>
            </tr>
        }
    </tbody>
</table>
